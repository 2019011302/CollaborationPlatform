ery: {taskName:item.taskname}}\" >\r\n\t\t\t\t\t\t\t\t<img src=\"/修改蓝.png\" />\r\n\t\t\t\t\t\t\t</router-link>\r\n\t\t\t\t\t\t\t<img src=\"/删除红.png\" @click=\"isShow=true\" />\r\n\t\t\r\n\t\t\t\t\t\t</td>\r\n\t\t\t\t\t\t<mydelete v-show = \"isShow\" @isTrue = \"deleteTask(item.taskname)\" @isFalse = \"cancleDelete\"></mydelete>\r\n\t\t\t\t\t</tr>\r\n\t\t\t\t</tbody>\r\n\t\t\t</table>\r\n\t\t\t<div style=\"text-align: center;\">\r\n\t\t\t\t<br>\r\n\t\t\t\t共 {{mylength}} 个任务\r\n\t\t\t\t<br>\r\n\t\t\t</div>\r\n\t\t</div>\r\n\t\t<div v-else style=\"text-align: center;\">暂无任务</div>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\n\timport mydelete from './Delete.vue'\r\n\texport default {\r\n\t\tcomputed: {\r\n\t\t\tcomputedLength() {\r\n\t\t\t\treturn this.tablelist.length \r\n\t\t\t},\r\n\t\t\tgetTablelist(){\r\n\t\t\t\tvar list = this.tablelist.filter(item => (item.level.includes(this.mylevel)\r\n\t\t\t\t&& item.state.includes(this.mystate) && item.group.includes(this.mygroup)\r\n\t\t\t\t&& item.time.includes(this.mydate)\r\n\t\t\t\t))\r\n\t\t\t\tthis.mylength = list.length\r\n\t\t\t\treturn list \r\n\t\t\t}\r\n\t\t},\r\n\t\tcomponents:{\r\n\t\t\tmydelete\r\n\t\t},\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\tisShow : false,\r\n\t\t\t\tmylevel :'',\r\n\t\t\t\tmystate : '',\r\n\t\t\t\tmygroup : '',\r\n\t\t\t\tmydate  : '',\r\n\t\t\t\tmylength : 0 ,\r\n\t\t\t\t// value :this.projectName,\r\n\t\t\t\ttablelist:[]\r\n\t\t\t}\r\n\t\t},\r\n\t\tmounted(){\r\n\t\t\t//获得任务表数据  所有任务--根据项目名得到当前项目的任务\r\n\t\t\t//找到任务  先拿到所有任务\r\n\t\t\tthis.tablelist  =JSON.parse(localStorage.getItem('task')) || []\r\n\t\t\tconsole.log('初值'+this.tablelist[0].projectname)\r\n\t\t\t//console.log( '项目名'+ this.project)\r\n\t\t\tvar name = this.$route.query.projectName;\r\n\t\t\t// console.log('test' + name)\r\n\t\t\t//根据当前项目名筛选\r\n\t\t\tthis.tablelist = this.tablelist.filter(item =>item.projectname===name)\r\n\t\t\t //this.tablelist = this.tablelist.filter(item =>item.projectname==='科研开发')\r\n\t\t\tconsole.log('筛选后'+this.tablelist)\r\n\t\t},\r\n\t\tmethods:{\r\n\t\t\tdeleteTask(name) {\r\n\t\t\t\t//全部删除\r\n\t\t\t\t// localStorage.removeItem('task')\r\n\t\t\t\t// this.tablelist=JSON.parse(localStorage.getItem('task')) || []\t\r\n\t\t\r\n\t\t\t\t//this.tablelist.splice(index , 1)\r\n\t\t\t\t//根据任务名删除\r\n\t\t\t\t\r\n\t\t\t\t//先找到任务名所在的位置\r\n\t\t\t\t\r\n\t\t\t\t//this.tablelist\r\n\t\t\t\t//先找到任务名所在的位置\r\n\t\t\t\t//console.log(this.tablelist.length)\r\n\t\t\t\tvar i =0\r\n\t\t\t\tfor( i = 0 ; i<this.tablelist.length ; i++ ){\r\n\t\t\t\t\tif(this.tablelist[i].taskname ===name){\r\n\t\t\t\t\t\tthis.tablelist.splice(i ,1)\r\n\t\t\t\t\t\tlocalStorage.setItem('task',JSON.stringify(this.tablelist))\r\n\t\t\t\t\t\t//this.tablelist  =JSON.parse(localStorage.getItem('task')) || []\r\n\t\t\t\t\t\tbreak;\r\n\t\t\t\t\t}\r\n\t\t\t\t\t// else{\r\n\t\t\t\t\t// \talert('error')\r\n\t\t\t\t\t// }\r\n\t\t\t\t}\r\n\t\t\t\t//删除该位置的数值\r\n\t\t\t\t\r\n\t\t\t\t//保存\r\n\t\t\t\t\r\n\t\t\t\tthis.isShow = false\r\n\t\t\t},\r\n\t\t\tcancleDelete(){\r\n\t\t\t\tthis.isShow = false\r\n\t\t\t}\t\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style scoped>\r\n\t.table-head {\r\n\t\tpadding-right: 17px;\r\n\t\tbackground-color: #3A8EE6;\r\n\t\tcolor: #fff;\r\n\t}\r\n\r\n\t.table-body {\r\n\t\twidth: 100%;\r\n\t\theight: 300px;\r\n\t\toverflow: scroll;\r\n\t}\r\n\r\n\t.table-head table,\r\n\t.table-body table {\r\n\t\twidth: 100%;\r\n\t}\r\n\r\n\t/* .table-body table tr:nth-child(2n+1){background-color:#f2f2f2;} */\r\n\tth,\r\n\ttd {\r\n\t\ttext-align: center;\r\n\r\n\t\twidth: 120px;\r\n\t\theight: 45px;\r\n\t}\r\n\r\n\ttable {\r\n\t\tborder-collapse: collapse;\r\n\t}\r\n\r\n\ttd {\r\n\t\tborder: 1px solid #e9e9e9;\r\n\t}\r\n\timg{\r\n\t\twidth: 20px;\r\n\t\theight: 20px;\r\n\t}\r\n\tinput {\r\n\t\twidth: 120px;\r\n\t\tmargin-right: 30px;\r\n\t}\r\n</style>\n"]}]}